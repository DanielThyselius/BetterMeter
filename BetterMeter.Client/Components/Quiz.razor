<div class="quiz-container">
    @Description

    <div class="quiz-content">
        @ChildContent
    </div>

    <p>end</p>
</div>

<style>
    .quiz-container {
        border: 1px solid black;
        padding: 10px;
        margin: 10px;
    }
    .quiz-content{
        background-color: #94c7a1;
        padding: 10px;
        border-radius: 10px;
    }
</style>

@code {
    [Parameter]
    public RenderFragment ChildContent { get; set; }

    [Parameter]
    public RenderFragment Description { get; set; }
}